    <style>
      body {
        font-family: "Inter", sans-serif;
      }
      .card-img-top {
        height: 180px;
        object-fit: cover;
        border-radius: 0.375rem;
      }
      .navbar-brand i {
        background: white;
        color: #2a64e8;
        border-radius: 0.125rem;
        padding: 0.125rem 0.25rem;
        font-size: 1.25rem;
        font-weight: 700;
      }
      .card {
        border: 1px solid #d1d5db;
        box-shadow: 0 1px 3px rgb(0 0 0 / 0.1);
        border-radius: 0.5rem;
      }
      .card-title {
        font-size: 0.75rem;
        font-weight: 600;
        margin-bottom: 0;
      }
      .ellipsis-icon {
        font-size: 0.75rem;
        color: #4b5563;
        cursor: pointer;
      }
      /* Remove border and background from card-footer */
      .card-footer {
        background-color: transparent;
        border: none;
        padding-top: 0.25rem;
        padding-bottom: 0.75rem;
        padding-left: 1rem;
        padding-right: 1rem;
      }
      /* Smaller audio controls */
      .card-footer audio {
        width: 100%;
        height: 28px; /* smaller height */
      }
    </style>
    <link
      href="https://fonts.googleapis.com/css2?family=Inter:wght@400;600&display=swap"
      rel="stylesheet"
    />
